{
  "name": "react-ts-boilerplate",
  "version": "1.0.0",
  "description": "React + Typescript + Webpack + SCSS Modules boilerplate",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darkonica/react-ts-boilerplate.git"
  },
  "author": "",
